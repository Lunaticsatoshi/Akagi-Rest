    "@aws-cdk/core": "^1.204.0",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/types": "^19.5.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
